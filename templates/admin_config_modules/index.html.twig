{% extends 'base.html.twig' %}

{% block title %}AdminConfigModules index{% endblock %}

{% block body %}
    <h1>AdminConfigModules index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>ModuleName</th>
                <th>ModuleDescription</th>
                <th>MinRole</th>
                <th>ModuleCreate</th>
                <th>ModuleBy</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for admin_config_module in admin_config_modules %}
            <tr>
                <td>{{ admin_config_module.id }}</td>
                <td>{{ admin_config_module.moduleName }}</td>
                <td>{{ admin_config_module.moduleDescription }}</td>
                <td>{{ admin_config_module.minRole }}</td>
                <td>{{ admin_config_module.moduleCreate ? admin_config_module.moduleCreate|date('Y-m-d H:i:s') : '' }}</td>
                <td>{{ admin_config_module.moduleBy }}</td>
                <td>
                    <a href="{{ path('app_admin_config_modules_show', {'id': admin_config_module.id}) }}">show</a>
                    <a href="{{ path('app_admin_config_modules_edit', {'id': admin_config_module.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="7">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_admin_config_modules_new') }}">Create new</a>
{% endblock %}
