{% extends 'base.html.twig' %}

{% block title %}Scripting{% endblock %}

{% block body %}
    <h1>Scripting</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ scripting.id }}</td>
            </tr>
            <tr>
                <th>Scriptname</th>
                <td>{{ scripting.scriptname }}</td>
            </tr>
            <tr>
                <th>ScriptDevstart</th>
                <td>{{ scripting.scriptDevstart }}</td>
            </tr>
            <tr>
                <th>ScriptUpdate</th>
                <td>{{ scripting.scriptUpdate }}</td>
            </tr>
            <tr>
                <th>ScriptReadme</th>
                <td>{{ scripting.scriptReadme }}</td>
            </tr>
            <tr>
                <th>Language</th>
                <td>{{ scripting.language }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_scripting_index') }}">back to list</a>

    <a href="{{ path('app_scripting_edit', {'id': scripting.id}) }}">edit</a>

    {{ include('scripting/_delete_form.html.twig') }}
{% endblock %}
