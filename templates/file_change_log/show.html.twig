{% extends 'base.html.twig' %}

{% block title %}FileChangeLog{% endblock %}

{% block body %}
    <h1>FileChangeLog</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ file_change_log.id }}</td>
            </tr>
            <tr>
                <th>FilePath</th>
                <td>{{ file_change_log.filePath }}</td>
            </tr>
            <tr>
                <th>EventType</th>
                <td>{{ file_change_log.eventType }}</td>
            </tr>
            <tr>
                <th>EventTime</th>
                <td>{{ file_change_log.eventTime ? file_change_log.eventTime|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>OldFilePath</th>
                <td>{{ file_change_log.oldFilePath }}</td>
            </tr>
            <tr>
                <th>FileSize</th>
                <td>{{ file_change_log.fileSize }}</td>
            </tr>
            <tr>
                <th>User</th>
                <td>{{ file_change_log.user }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_file_change_log_index') }}">back to list</a>

    <a href="{{ path('app_file_change_log_edit', {'id': file_change_log.id}) }}">edit</a>

    {{ include('file_change_log/_delete_form.html.twig') }}
{% endblock %}
