{% extends 'base.html.twig' %}

{% block title %}UserLoginHistory{% endblock %}

{% block body %}
    <h1>UserLoginHistory</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ user_login_history.id }}</td>
            </tr>
            <tr>
                <th>LoginAt</th>
                <td>{{ user_login_history.loginAt ? user_login_history.loginAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>IpAddress</th>
                <td>{{ user_login_history.ipAddress }}</td>
            </tr>
            <tr>
                <th>UserAgent</th>
                <td>{{ user_login_history.userAgent }}</td>
            </tr>
            <tr>
                <th>Location</th>
                <td>{{ user_login_history.location }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_user_login_history_index') }}">back to list</a>

    <a href="{{ path('app_user_login_history_edit', {'id': user_login_history.id}) }}">edit</a>

    {{ include('user_login_history/_delete_form.html.twig') }}
{% endblock %}
