{% extends 'base.html.twig' %}

{% block title %}SymlinkCreator{% endblock %}

{% block body %}
    <h1>SymlinkCreator</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ symlink_creator.id }}</td>
            </tr>
            <tr>
                <th>SourcePath</th>
                <td>{{ symlink_creator.sourcePath }}</td>
            </tr>
            <tr>
                <th>SourceDestination</th>
                <td>{{ symlink_creator.sourceDestination }}</td>
            </tr>
            <tr>
                <th>SymlinkCreated</th>
                <td>{{ symlink_creator.symlinkCreated ? symlink_creator.symlinkCreated|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>SymlinkCreatedBy</th>
                <td>{{ symlink_creator.symlinkCreatedBy }}</td>
            </tr>
            <tr>
                <th>SymlinkStatus</th>
                <td>{{ symlink_creator.symlinkStatus }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_symlink_creator_index') }}">back to list</a>

    <a href="{{ path('app_symlink_creator_edit', {'id': symlink_creator.id}) }}">edit</a>

    {{ include('symlink_creator/_delete_form.html.twig') }}
{% endblock %}
