{% extends 'base.html.twig' %}

{% block title %}CostCenter{% endblock %}

{% block body %}
    <h1>CostCenter</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ cost_center.id }}</td>
            </tr>
            <tr>
                <th>Code</th>
                <td>{{ cost_center.code }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ cost_center.name }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ cost_center.description }}</td>
            </tr>
            <tr>
                <th>Active</th>
                <td>{{ cost_center.active ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_cost_center_index') }}">back to list</a>

    <a href="{{ path('app_cost_center_edit', {'id': cost_center.id}) }}">edit</a>

    {{ include('cost_center/_delete_form.html.twig') }}
{% endblock %}
