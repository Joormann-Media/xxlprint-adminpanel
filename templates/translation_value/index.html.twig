{% extends 'base.html.twig' %}

{% block title %}TranslationValue index{% endblock %}

{% block body %}
    <h1>TranslationValue index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Value</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for translation_value in translation_values %}
            <tr>
                <td>{{ translation_value.id }}</td>
                <td>{{ translation_value.value }}</td>
                <td>
                    <a href="{{ path('app_translation_value_show', {'id': translation_value.id}) }}">show</a>
                    <a href="{{ path('app_translation_value_edit', {'id': translation_value.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_translation_value_new') }}">Create new</a>
{% endblock %}
