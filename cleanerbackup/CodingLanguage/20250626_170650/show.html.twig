{% extends 'base.html.twig' %}

{% block title %}CodingLanguage{% endblock %}

{% block body %}
    <h1>CodingLanguage</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ coding_language.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ coding_language.name }}</td>
            </tr>
            <tr>
                <th>Logo</th>
                <td>{{ coding_language.logo }}</td>
            </tr>
            <tr>
                <th>Year</th>
                <td>{{ coding_language.year }}</td>
            </tr>
            <tr>
                <th>LanguageType</th>
                <td>{{ coding_language.languageType }}</td>
            </tr>
            <tr>
                <th>Extension</th>
                <td>{{ coding_language.extension }}</td>
            </tr>
            <tr>
                <th>Developer</th>
                <td>{{ coding_language.developer }}</td>
            </tr>
            <tr>
                <th>Paradigm</th>
                <td>{{ coding_language.paradigm }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_coding_language_index') }}">back to list</a>

    <a href="{{ path('app_coding_language_edit', {'id': coding_language.id}) }}">edit</a>

    {{ include('coding_language/_delete_form.html.twig') }}
{% endblock %}
