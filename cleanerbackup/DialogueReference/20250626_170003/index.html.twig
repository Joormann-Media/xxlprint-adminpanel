{% extends 'base.html.twig' %}

{% block title %}DialogueReference index{% endblock %}

{% block body %}
    <h1>DialogueReference index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>RoomId</th>
                <th>DialogId</th>
                <th>DialogText</th>
                <th>DialogLang</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for dialogue_reference in dialogue_references %}
            <tr>
                <td>{{ dialogue_reference.id }}</td>
                <td>{{ dialogue_reference.roomId }}</td>
                <td>{{ dialogue_reference.dialogId }}</td>
                <td>{{ dialogue_reference.dialogText }}</td>
                <td>{{ dialogue_reference.dialogLang }}</td>
                <td>
                    <a href="{{ path('app_dialogue_reference_show', {'id': dialogue_reference.id}) }}">show</a>
                    <a href="{{ path('app_dialogue_reference_edit', {'id': dialogue_reference.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="6">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_dialogue_reference_new') }}">Create new</a>
{% endblock %}
